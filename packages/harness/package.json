{
  "name": "@oneclaw/harness",
  "version": "0.1.0",
  "description": "Core infrastructure - database-agnostic schemas, stores, identity, billing for multi-tenant AI agents",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./gmail/encryption": {
      "import": "./src/gmail/encryption.ts",
      "require": "./src/gmail/encryption.ts",
      "types": "./src/gmail/encryption.ts"
    },
    "./gmail/client": {
      "import": "./src/gmail/client.ts",
      "require": "./src/gmail/client.ts",
      "types": "./src/gmail/client.ts"
    }
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@hono/node-server": "^1.13.0",
    "cheerio": "^1.2.0",
    "dotenv": "^17.3.1",
    "googleapis": "^140.0.0",
    "hono": "^4.0.0",
    "nanoid": "^5.0.0",
    "playwright": "^1.58.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@supabase/supabase-js": {
      "optional": true
