{
  "name": "oneclaw",
  "version": "0.1.0",
  "description": "OneClaw - Modular agent operating system with durable workflow execution",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/executors/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "node apps/api/dist/index.js",
    "lint": "turbo lint",
    "test": "turbo test",
    "clean": "turbo clean && rm -rf node_modules",
    "harness:dev": "pnpm --filter @oneclaw/harness dev",
    "oneclaw": "pnpm --filter @oneclaw/cli run build && node packages/cli/dist/index.js",
    "node:build": "pnpm --filter @oneclaw/node-runtime build",
    "node:dev": "pnpm --filter @oneclaw/node-runtime dev",
    "node:test": "pnpm --filter @oneclaw/executor-http test"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.43.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0",
    "prettier": "^3.2.0",
    "eslint": "^8.56.0",
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "dotenv": "^16.4.0"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "eslint-scope": "^7.2.2",
    "eslint-visitor-keys": "^3.4.3"
  },
  "keywords": ["ai", "agents", "workflows", "restate", "durable-execution"],
  "author": "",
  "license": "ISC"
}
